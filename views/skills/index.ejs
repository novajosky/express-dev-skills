<%- include('../partials/header') %>

<h1>Skills</h1>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
        <a href="/skills/<%= s.id %>">
            <%= s.id %>
        </a>

        <%= s.skill %>
            -
            <%= s.has ? 'yes' : 'no' %>

    </li>
  <% }); %>
</ul>

<%- include('../partials/footer') %>